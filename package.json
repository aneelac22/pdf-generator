    "testEnvironment": "jsdom",
    "testRunner": "/home/martin/insights/crc-pdf-generator/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
